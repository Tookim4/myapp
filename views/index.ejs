<!DOCTYPE html>
<html lang="en">
<%- include('partials/header') %>
<body>
    <%- include('partials/nav') %>

    <h1><%= title %></h1>

    <% if (blogs.length > 0) { %>
        <% blogs.forEach(blog => { %>
            <h3><%= blog.title %></h3>
            <p><%= blog.snippet%></p>
            <% }) %>
    <% } else { %>
        <h2>No Blog Found</h2>
    <% } %>

</body>
</html>